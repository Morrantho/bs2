{
	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "bs_dbg",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/bs",
			// "args": [ "test.bs" ],
			"args": [ ],
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands":
			[
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}